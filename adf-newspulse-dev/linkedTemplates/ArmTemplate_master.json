{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-newspulse-dev"
		},
		"ls_blob_raw_dev_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_raw_dev'"
		},
		"ls_databricks_newspulse_dev_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks_newspulse_dev'"
		},
		"ls_sql_newspulse_dev_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_newspulse_dev'"
		},
		"ls_databricks_newspulse_dev_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0829-214831-jfgeylz6"
		},
		"ls_sql_newspulse_dev_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-newspulse-dev.database.windows.net"
		},
		"ls_sql_newspulse_dev_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb-newspulse-dev"
		},
		"ls_sql_newspulse_dev_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-newspulse-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blob_raw_dev_connectionString": {
						"value": "[parameters('ls_blob_raw_dev_connectionString')]"
					},
					"ls_databricks_newspulse_dev_accessToken": {
						"value": "[parameters('ls_databricks_newspulse_dev_accessToken')]"
					},
					"ls_sql_newspulse_dev_password": {
						"value": "[parameters('ls_sql_newspulse_dev_password')]"
					},
					"ls_databricks_newspulse_dev_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_newspulse_dev_properties_typeProperties_existingClusterId')]"
					},
					"ls_sql_newspulse_dev_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_newspulse_dev_properties_typeProperties_server')]"
					},
					"ls_sql_newspulse_dev_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_newspulse_dev_properties_typeProperties_database')]"
					},
					"ls_sql_newspulse_dev_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_newspulse_dev_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}